
double alanHesapla(double kisaKenar, double uzunKenar) {

  return kisaKenar * uzunKenar;
}

void main() {

  double kisaKenar = 4.76;
  double uzunKenar = 9.54;


  double alan = alanHesapla(kisaKenar, uzunKenar);


  print("Dikdörtgenin alanı: $alan");
}


void main() {
 
  print(carp(5, 3)); // Sonuc: 20
}


int carp(int a, int b) {

  int result = a;

  // b-1 kez 2 ile çarpma işlemi
  for (int i = 0; i < b - 1; i++) {
    result *= 2;
  }


  return result;
}
